    <class>br.com.core.entity.UsuariosTO</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <validation-mode>NONE</validation-mode>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/bdg?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="marcelocaser"/>
